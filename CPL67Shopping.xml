<?xml version="1.0" encoding="utf-8"?>
<Containers>
    <Beta>False</Beta>
    <Category>Productivity: Tools</Category>
    <Name>CPL67Shopping</Name>
    <Project>https://github.com/JonaMata/CPL67Shopping/</Project>
    <Description>
        The CPL67Shopping list application
    </Description>
    <Registry>https://registry.hub.docker.com/u/limetech/plex/</Registry>
    <GitHub>https://github.com/limetech/dockerapp-plex</GitHub>
    <Repository>https://github.com/JonaMata/CPL67Shopping/</Repository>
    <Support>https://github.com/JonaMata/CPL67Shopping/</Support>
    <TemplateURL>https://raw.githubusercontent.com/JonaMata/CPL67Shopping/master/CPL67Shopping.xml</TemplateURL>
    <Privileged>false</Privileged>
    <Network>webaccess</Network>
    <Config Name="Application URL" Target="ROOT_URL" Default="http://localhost" Mode="" Description="The url on which the application will be served" Type="Variable" Display="always" Required="true" Mask="false"/>
